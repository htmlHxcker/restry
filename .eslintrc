{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "jsx-a11y/label-has-associated-control": [
            1,
            {
                "required": {
                    "some": [
                        "nesting",
                        "id"
                    ]
                }
            }
        ]
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "document": false
    }
}